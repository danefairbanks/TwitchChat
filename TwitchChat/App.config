<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- Create a client id for twitch application that redirects to http://dummy -->
    <add key="twitch-api:client-id" value="insert your client id here"/>
    <!-- Maximum messages to show per chat (channel and whispers) -->
    <add key="twitch-api:maxmessages" value="150"/>
  </appSettings>
  <system.net>
    <defaultProxy useDefaultCredentials="true"></defaultProxy>
  </system.net>
</configuration>